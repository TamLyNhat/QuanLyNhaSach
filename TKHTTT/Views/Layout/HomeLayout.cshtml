    <!--Footer-->

    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-4">
                    <h1 id="text-footer">DỊCH VỤ</h1>
                    <h1 class="title-footer"><i><img src="~/Content/image/email.png" class="fiximage"></i>DoAn@gmail.com</h1>
